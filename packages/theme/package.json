{
    "name": "@oneweek-react/theme",
    "version": "0.0.1",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.mjs",
            "types": "./dist/cjs/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "local:pkg": "rollup -w -c",
        "build-declarations": "tsc --declaration --emitDeclarationOnly --declarationDir dist/cjs",
        "build:pkg": "pnpm clean && pnpm build-declarations && rollup -c",
        "clean": "rm -rf dist"
    },
    "author": "2-one-week",
    "license": "ISC",
    "devDependencies": {
        "@oneweek-share/rollup-config": "workspace:*",
        "@oneweek-share/tsconfig": "workspace:*",
        "@types/react": ">= 18.0.0",
        "@types/react-dom": ">= 18.0.0",
        "@types/styled-components": "^5.1.26",
        "rollup": "^2.79.1"
    },
    "dependencies": {
        "react": ">= 18.0.0",
        "react-dom": ">= 18.0.0",
        "styled-components": "6.0.0-rc.3",
        "typescript": "^5.1.3"
    }
}
