{
    "name": "@oneweek/util",
    "version": "0.0.1",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.mjs",
            "types": "./dist/cjs/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "local:pkg": "rollup -w -c",
        "build-declarations": "tsc --declaration --emitDeclarationOnly --declarationDir dist/cjs",
        "build:pkg": "pnpm clean && pnpm build-declarations && rollup -c",
        "clean": "rm -rf dist"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "rollup": "^2.79.1",
        "@types/node": "^18.0.3",
        "concurrently": "^7.6.0",
        "@oneweek-share/tsconfig": "workspace:*",
        "@oneweek-share/rollup-config": "workspace:*"
    }
}
