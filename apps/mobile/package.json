{
    "name": "mobile",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "aos": "react-native run-android",
        "ios": "arch -x86_64 react-native run-ios",
        "local": "pnpm delete-watchman && react-native start",
        "test": "jest",
        "link:aos": "adb start-server",
        "delete-watchman": "watchman watch-del '/Users/user/Desktop/oneweek/oneweek-template' ; watchman watch-project '/Users/user/Desktop/oneweek/oneweek-template'",
        "clean": "rm -rf dist",
        "clean:aos": "cd android && ./gradlew clean && cd ..",
        "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && npx pod-install"
    },
    "dependencies": {
        "@gorhom/portal": "^1.0.14",
        "@oneweek-react/hook": "workspace:*",
        "@oneweek-react/theme": "workspace:*",
        "@oneweek/fetch": "workspace:*",
        "@oneweek/util": "workspace:*",
        "@react-native-async-storage/async-storage": "^1.17.11",
        "@react-native-firebase/analytics": "^16.5.0",
        "@react-native-firebase/app": "^16.5.0",
        "@react-native-firebase/crashlytics": "^16.5.0",
        "@react-navigation/bottom-tabs": "^6.4.0",
        "@react-navigation/elements": "^1.3.15",
        "@react-navigation/native": "^6.1.1",
        "@react-navigation/native-stack": "^6.9.1",
        "axios": "^1.2.3",
        "lottie-react-native": "^5.1.4",
        "query-string": "^8.1.0",
        "react": "^18.2.0",
        "react-native": "0.70.5",
        "react-native-config": "^1.5.0",
        "react-native-device-info": "^10.3.0",
        "react-native-exception-handler": "^2.10.10",
        "react-native-gesture-handler": "^2.8.0",
        "react-native-progress": "^5.0.0",
        "react-native-reanimated": "^2.13.0",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-svg": "^13.6.0",
        "react-native-webview": "^11.23.1",
        "styled-components": "6.0.0-rc.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.19.0",
        "@oneweek-share/tsconfig": "workspace:*",
        "@react-native-community/cli-platform-android": "^8.0.5",
        "@react-native-community/cli-platform-ios": "^8.0.4",
        "@rnx-kit/babel-preset-metro-react-native": "^1.1.4",
        "@rnx-kit/metro-config": "^1.3.6",
        "@rnx-kit/metro-resolver-symlinks": "^0.1.28",
        "@types/react": "^18.0.16",
        "@types/react-native": "^0.69.13",
        "babel-jest": "^26.6.3",
        "babel-plugin-root-import": "^6.6.0",
        "jest": "^26.6.3",
        "jsc-android": "^250230.2.1",
        "metro-react-native-babel-preset": "^0.72.1",
        "promise": "^8.3.0",
        "react-native-gradle-plugin": "^0.71.1"
    },
    "jest": {
        "preset": "react-native"
    }
}
